{
  "$schema": "http://json.schemastore.org/template",
  "author": "Calabonga SOFT",
  "classifications": [
    "Common",
    "Desktop",
    "Windows"
  ],
  "name": "WPF Application with MVVM",
  "description": "This template for WPF application with MVVM components you can use for new application. In template already everything prepared for your developing start quickly.",
  "identity": "Calabonga.Wpf.Mvvm.Template",
  "shortName": "wpfmvvm",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "defaultName": "Calabonga.Wpf.Mvvm1",
  "sourceName": "Calabonga.Wpf.Mvvm",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net9.0"
        }
      ],
      "defaultValue": "net9.0"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**",
            ".idea/**",
            "_ReSharper.Caches/**",
            ".template_config/**"
          ]
        }
      ]
    }
  ]
}